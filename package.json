{
  "name": "vue-threejs",
  "version": "0.2.0-alpha.1",
  "description": "Three.js bindings for Vue",
  "author": "Fritz Lin <uxfritz@163.com>",
  "repository": "https://github.com/fritx/vue-threejs",
  "license": "MIT",
  "main": "./lib/VueThreejs.common.js",
  "unpkg": "./lib/VueThreejs.umd.min.js",
  "scripts": {
    "demo:deploy": "gh-pages -d dist",
    "demo:dev": "vue-cli-service serve ./examples/main.js",
    "demo:build": "vue-cli-service build ./examples/main.js",
    "build": "cross-env TARGET=lib vue-cli-service build ./src/index.js --target=lib --dest=lib --name=VueThreejs",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run test:unit",
    "lint": "vue-cli-service lint",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "three": ">=0.133.1",
    "vue": ">=2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.14",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "cross-env": "^7.0.3",
    "dat.gui": "^0.7.7",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.20.0",
    "gh-pages": "^3.2.3",
    "oimo": "^1.0.9",
    "three": "^0.133.1",
    "vue": "^2.6.4",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "lib/VueThreejs.common.js",
    "lib/VueThreejs.common.js.map",
    "lib/VueThreejs.umd.js",
    "lib/VueThreejs.umd.js.map",
    "lib/VueThreejs.umd.min.js",
    "lib/VueThreejs.umd.min.js.map"
  ]
}
